@model SkillsShowcaseMVC.Models.EmployeeSecretKeyViewModel

@if (Model != null)
{
    <div>
        <p><strong>Employee ID:</strong> @Model.Id</p>
        <p><strong>Employee Name:</strong> @Model.EmployeeName</p>
        <p><strong>Secret Key:</strong> @Model.SecretKey</p>
    </div>
}
else
{
    <p>No secret key found for this employee.</p>
}